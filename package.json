{
  "name": "apx",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./vite-plugin": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:js": "bun build src/apx/plugins/index.ts --format esm --target node --outfile dist/index.js --sourcemap",
    "build": "bun run build:js && bun run build:types",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.7.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "orval": "^7.13.2",
    "vite": "^7.1.9"
  }
}
