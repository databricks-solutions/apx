{
  "name": "apx",
  "version": "0.1.1",
  "type": "module",
  "main": "./plugin-dist/index.js",
  "module": "./plugin-dist/index.js",
  "types": "./plugin-dist/index.d.ts",
  "exports": {
    "./vite-plugin": {
      "types": "./plugin-dist/index.d.ts",
      "import": "./plugin-dist/index.js",
      "default": "./plugin-dist/index.js"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "./plugin-dist",
    "README.md"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly --outDir ./plugin-dist",
    "build:js": "bun build src/apx/plugins/index.ts --format esm --minify --target node --outdir ./plugin-dist --sourcemap --external orval --external vite",
    "build": "bun run build:js && bun run build:types",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.15",
    "@tanstack/router-plugin": "^1.133.21",
    "@types/bun": "latest",
    "@types/node": "^24.7.2",
    "@vitejs/plugin-react": "^5.0.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "orval": "^7.13.2",
    "smol-toml": "^1.4.2",
    "vite": "^7.1.9"
  }
}
